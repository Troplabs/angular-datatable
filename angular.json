{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-datatable": {
      "projectType": "library",
      "root": "projects/angular-datatable",
      "sourceRoot": "projects/angular-datatable/src",
      "prefix": "mf",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/angular-datatable/tsconfig.lib.json",
            "project": "projects/angular-datatable/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-datatable/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/angular-datatable/src/test.ts",
            "tsConfig": "projects/angular-datatable/tsconfig.spec.json",
            "karmaConfig": "projects/angular-datatable/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "packageManager": "npm"
  },
  "defaultProject": "angular-datatable"
}
